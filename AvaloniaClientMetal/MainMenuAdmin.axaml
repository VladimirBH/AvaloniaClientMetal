<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="300"
        x:Class="AvaloniaClientMetal.MainMenuAdmin"
        Title="Админ панель"
        WindowStartupLocation="CenterScreen"
        Width="300" Height="300" CanResize="False"
        Closing="Window_OnClosing">
    <Grid Margin="15" RowDefinitions="1*, 1*, 2*" ColumnDefinitions="1*">
        <Button Name="ButtonUsers" Classes="List" Click="ButtonUsers_OnClick" Grid.Row="0" HorizontalAlignment="Center" HorizontalContentAlignment="Center" Width="200">Пользователи</Button>
        <Button Name="ButtonRoles" Classes="List" Click="ButtonRoles_OnClick" Grid.Row="1" HorizontalAlignment="Center" HorizontalContentAlignment="Center" Width="200">Роли</Button>
        <Button Name="ButtonSubmit" Classes="List" Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Center" Width="200" HorizontalContentAlignment="Center">Выйти</Button>
    </Grid>
</Window>
